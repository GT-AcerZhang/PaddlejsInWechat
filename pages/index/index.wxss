page {
  background-color: #5fc883;
}

.round-click{
  height: 200rpx;
  width: 200rpx;
  background-color: #46c372;
  border-radius: 100%;
  position: fixed;
  bottom: -70rpx;
  right: -70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9;
}
.round-click navigator{
  font-size: 32rpx;
  max-width: 80rpx;
  color: #fff;
  text-align: center;
}

.content {
  position: fixed;
  height: 90%;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.bt {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
}

image {
  width: 400rpx;
  height: 400rpx;
}